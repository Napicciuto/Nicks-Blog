<%= semantic_form_for @author, url: { action: :create } do |f| %>
	<%= f.input :first_name, label: "First Name" %>
	<%= f.input :last_name, label: "Last Name" %>
	<%= f.input :email %>
												
	<%= f.input :address, label: "Address" %>
	<%= f.input :city %>
	<%= f.input :state %>
	<%= f.input :zip %>
	
	<%= f.input :company %>
	
	<%= f.input :password %>
	<%= f.input :password_confirmation, label: "Confirm Password" %>
	<%= f.submit %>
	
<% end %>